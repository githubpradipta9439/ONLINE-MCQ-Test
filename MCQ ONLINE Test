  var q9=document.quiz.question9.value;
  var q10=document.quiz.question10.value;
  var reult=document.getElementById('result');
  var quiz=document.getElementById("quiz");
  
  if(q1=="4000"){c++}
  if(q2=="25.5"){c++}
  if(q3=="552"){c++}
  if(q4=="2"){c++}
  if(q5=="15"){c++}
  if(q6=="3:5"){c++}
  if(q7=="20%"){c++}
  if(q8=="0.00006"){c++}
  if(q9=="600"){c++}
  if(q10=="15"){c++}
  
  document.write("YOUR  RESULT  IS :  ");
  document.write(c);
  
}

</script>
